<?xml version="1.0" encoding="UTF-8"?>
<!-- example of a fully specified provenance query -->

<pquery>
	<workflow name="Workflow1">
		<runs><!--  this defines the global scope. if omitted, it defaults to the latest run -->
			<run id="run1" /><!--  will be ignored  -->
			<run id="ae1e2b6b-3bc5-4c93-a250-c4dd0210c3b3" /><!--  meant to be valid -->
			<range from="2009-10-08" />
		</runs>
	</workflow>
</pquery>