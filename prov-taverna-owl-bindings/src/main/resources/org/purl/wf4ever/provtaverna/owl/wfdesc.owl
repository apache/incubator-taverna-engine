    <!-- http://purl.org/wf4ever/wfdesc#hasArtifact -->

    <owl:ObjectProperty rdf:about="&wfdesc;hasArtifact">
        <comment xml:lang="en">This property associates a parameter with an artifact. The artifact is used to describe the parameter, e.g., its data type, data structure, etc.</comment>
        <range rdf:resource="&wfdesc;Artifact"/>
        <domain rdf:resource="&wfdesc;Parameter"/>
        <subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/wf4ever/wfdesc#hasDataLink -->

    <owl:ObjectProperty rdf:about="&wfdesc;hasDataLink">
        <comment xml:lang="en">This property is used to identify the data links that belong to a given workflow template.</comment>
        <range rdf:resource="&wfdesc;DataLink"/>
        <domain rdf:resource="&wfdesc;WorkflowTemplate"/>
        <subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/wf4ever/wfdesc#hasInput -->

    <owl:ObjectProperty rdf:about="&wfdesc;hasInput">
        <comment xml:lang="en">This object property is used to specify the input parameter of a given process.</comment>
        <range rdf:resource="&wfdesc;Input"/>
        <domain rdf:resource="&wfdesc;Process"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/wf4ever/wfdesc#hasOutput -->

    <owl:ObjectProperty rdf:about="&wfdesc;hasOutput">
        <comment xml:lang="en">This object property is used to specify the output parameter of a given process.
</comment>
        <range rdf:resource="&wfdesc;Output"/>
        <domain rdf:resource="&wfdesc;Process"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/wf4ever/wfdesc#hasSink -->

    <owl:ObjectProperty rdf:about="&wfdesc;hasSink">
        <comment xml:lang="en">This property is used to specify the input parameter that acts as a sink from a given data Link.</comment>
        <domain rdf:resource="&wfdesc;DataLink"/>
        <range rdf:resource="&wfdesc;Input"/>
        <subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/wf4ever/wfdesc#hasSource -->

    <owl:ObjectProperty rdf:about="&wfdesc;hasSource">
        <comment xml:lang="en">This property is used to specify the output parameter that acts as a source to a given data link.</comment>
        <domain rdf:resource="&wfdesc;DataLink"/>
        <range rdf:resource="&wfdesc;Output"/>
        <subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/wf4ever/wfdesc#hasSubProcess -->

    <owl:ObjectProperty rdf:about="&wfdesc;hasSubProcess">
        <comment xml:lang="en">This object property is used to capture nesting between processes. Specifically, it is used to specify that the given process contains a given process.

</comment>
        <range rdf:resource="&wfdesc;Process"/>
        <domain rdf:resource="&wfdesc;WorkflowTemplate"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/wf4ever/wfdesc#hasSubWorkflow -->

    <owl:ObjectProperty rdf:about="&wfdesc;hasSubWorkflow">
        <comment xml:lang="en">This object property associates a workflow template with another workflow template, specifying that the former contains the latter.</comment>
        <range rdf:resource="&wfdesc;WorkflowTemplate"/>
        <domain rdf:resource="&wfdesc;WorkflowTemplate"/>
        <subPropertyOf rdf:resource="&wfdesc;hasSubProcess"/>
    </owl:ObjectProperty>
    


